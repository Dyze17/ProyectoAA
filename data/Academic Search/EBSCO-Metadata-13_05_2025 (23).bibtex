 year      = "2024",
 issn      = "2045-2322",
 journal   = "Scientific Reports",
 keywords  = "OPTIMIZATION algorithms; COST functions; QUANTUM computing; CIRCUIT complexity; QUANTUM computers; NOISE",
 pages     = "1-11",
 note      = "",
}

