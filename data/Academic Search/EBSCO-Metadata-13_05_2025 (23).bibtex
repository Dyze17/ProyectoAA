@article{18230331320250119,
 abstract  = "The soilborne pathogen Fusarium oxysporum f. sp. cubense tropical race 4 (Foc TR4) is currently devastating banana production worldwide. Once introduced, it is not possible to eradicate the pathogen from soils where it can survive for decades. The only management option available then is to replace Foc TR4-susceptible with -resistant varieties. Timely detection of the pathogen, however, is an important strategy to prevent the introduction of Foc TR4 into new areas and prevent its spread from infested sites. In this study, a single-tube detection technique was developed by combining recombinase polymerase amplification (RPA) and clustered regularly interspaced short palindromic repeats (CRISPR)/Cas12a technology (RPA-Cas12a) for detection of Foc TR4. The RPA-Cas12a assay was conducted isothermally, had a sensitivity of up to 10 fg target DNA and did not cross react with any of the 76 non-target isolates included in the specificity testing. The RPA-Cas12a assay detected Foc TR4 from nat",
 author    = "Matthews, Megan Ceris and van der Linden, Jos and Robène, Isabelle and Rozsasi, Samuel and Coetzee, Beatrix and Campa, Manuela and Burger, Johan and Akwuruoha, Uzoma Nobel and Madufor, Ndubuisi Johnkennedy and Perold, Willem and Opara, Umezuruike Linus and Viljoen, Altus and Mostert, Diane",
 number    = "1",
 title     = "A combined recombinase polymerase amplification CRISPR/Cas12a assay for detection of Fusarium oxysporum f. sp. cubense tropical race 4.",
 volume    = "15",
 url       = "https://research.ebsco.com/linkprocessor/plink?id=e2f176b1-b358-3fb3-972b-c5a5c0c19e75",
 year      = "2025",
 issn      = "2045-2322",
 journal   = "Scientific Reports",
 keywords  = "FUSARIUM oxysporum; FUSARIUM wilt of banana; WILT diseases; BLUE light; FUSARIOSIS",
 pages     = "1-12",
 note      = "",
}

