 number    = "9",
 title     = "A new lineage nomenclature to aid genomic surveillance of dengue virus.",
 volume    = "22",
 url       = "https://research.ebsco.com/linkprocessor/plink?id=0121cb92-497d-367e-8534-49fbdeddce2d",
 year      = "2024",
 issn      = "1544-9173",
 journal   = "PLoS Biology",
 keywords  = "DENGUE viruses; EPIDEMICS; GENOTYPES; CLASSIFICATION",
 pages     = "1-27",
 note      = "",
}

