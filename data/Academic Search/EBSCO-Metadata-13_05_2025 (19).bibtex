 journal   = "Interactive Learning Environments",
 keywords  = "HIGH school students; ELECTROTEXTILES; LEARNING; PORTFOLIOS in education; COMMUNICATION",
 pages     = "284-301",
 note      = "",
}

@article{17538855620240201,
 abstract  = "Algorithmic systems that provide services to people by supporting or replacing human decision-making promise greater convenience in various areas. The opacity of these applications, however, means that it is not clear how much they truly serve their users. A promising way to address the issue of possible undesired biases consists in giving users control by letting them configure a system and aligning its performance with users' own preferences. However, as the present paper argues, this form of control over an algorithmic system demands an algorithmic literacy that also entails a certain way of making oneself knowable: users must interrogate their own dispositions and see how these can be formalized such that they can be translated into the algorithmic system. This may, however, extend already existing practices through which people are monitored and probed and means that exerting such control requires users to direct a computational mode of thinking at themselves.",
 author    = "König, Pascal D.",
 number    = "1",
 title     = "Challenges in enabling user control over algorithm-based services.",
 volume    = "39",
 url       = "https://research.ebsco.com/linkprocessor/plink?id=8787ac40-50ee-39db-8ae8-4661050828fd",
 year      = "2024",
 issn      = "0951-5666",
 journal   = "AI &amp; Society",
 keywords  = "ALGORITHMIC bias; DECISION making",
 pages     = "195-205",
 note      = "",
}

