@article{13316128620190201,
 abstract  = "Sorting is a process we encounter very often in everyday life. Additionally it is a fundamental operation in computer science. Having been one of the first intensely studied problems in computer science, many different sorting algorithms have been developed and analyzed. Although algorithms are often taught as part of the computer science curriculum in the context of a programming language, the study of algorithms and algorithmic thinking, including the design, construction and analysis of algorithms, has pedagogical value in mathematics education. This paper will provide an introduction to computational complexity and efficiency, without the use of a programming language. It will also describe how these concepts can be incorporated into the existing high school or undergraduate mathematics curriculum through a mathematical analysis of studentgenerated sorting algorithms.",
 author    = "Nasar, Audrey A.",
 number    = "1-3",
 title     = "A Mathematical Analysis of student-generated sorting algorithms.",
 volume    = "16",
 url       = "https://research.ebsco.com/linkprocessor/plink?id=8325b83e-3920-34b0-8844-c6c0d4055482",
 year      = "2019",
 issn      = "1551-3440",
 journal   = "Mathematics Enthusiast",
 keywords  = "MATHEMATICAL analysis; SORTING (Electronic computers); COMPUTATIONAL complexity; MATHEMATICS education; THOUGHT &amp; thinking",
 pages     = "315-330",
 note      = "",
}

