 year      = "2023",
 issn      = "0092-8674",
 journal   = "Cell (0092-8674)",
 keywords  = "SARS-CoV-2; AIR travel; SARS-CoV-2 Omicron variant; COUNTRY of origin (Commerce); AIR travelers; DATA analysis; SOUTH Africa; INDIA; Scheduled air transportation; Scheduled Passenger Air Transportation; Nonscheduled Chartered Passenger Air Transportation",
